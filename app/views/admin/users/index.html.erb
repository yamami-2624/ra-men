<div class="container">
    <div class="row">
        <div class="col-xs-7 col-xs-offset-1">
			<table class="table">
				<thead>
					<tr>
						<th>名前</th>
						<th>アドレス</th>
					</tr>
				</thead>
				<tbody>
					<% @users.each do |user| %>
						<tr>
							<td><%= link_to user.name, admin_user_path(user), class: "underline" %></td>
							<td><%= user.email %></td>
						</tr>
					<% end %>
				</tbody>
			</table>
		</div>
	</div>
</div>